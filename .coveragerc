[run]
source = core
data_file = reports/coverage/.coverage
omit =
    */tests/*
    */test_*
    */__main__.py
    core/coverage_fuzzer.py
    core/coverage_guided_fuzzer.py
    core/coverage_guided_mutator.py
    core/coverage_instrumentation.py
    core/corpus_manager.py
    core/coverage_correlation.py
    core/grammar_fuzzer.py
    core/mutation_minimization.py
    core/profiler.py
    core/target_runner.py
    core/enhanced_reporter.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod
precision = 2
skip_covered = False
skip_empty = True

[html]
directory = reports/coverage/htmlcov

[xml]
output = reports/coverage/coverage.xml
